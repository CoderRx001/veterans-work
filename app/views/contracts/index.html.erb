<h1>Contracts Index</h1>
<table class="table">
  <thead class="thead-inverse">
      <tr>
        <th> ID:  </th>
        <th> Customer Request ID: </th>
        <th> Company ID: </th>
        <th> Total Cost: </th>
        <th> Completion Date: </th>
        <th> Notes : </th>
      </tr>
  </thead>

  <tbody>
      <tr>
       <% @contracts.each do |contract| %>
        <td> <%= contract.id %> </td>
        <td>  <%= contract.quote.customer_request_id %> </td>
        <td>  <%= contract.quote.company_id %> </td>
        <td>  <%= contract.quote.total_cost_estimate %> </td>
        <td> <%= contract.quote.completion_date_estimate %> </td>
        <td> <%= contract.quote.notes %> </td>
<% end %>
      </tr>
  </tbody>
</table>
